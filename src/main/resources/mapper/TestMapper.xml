<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.deep.infra.persistence.sql.mapper.TestMapper">

    <select id="getTestModelById" resultType="com.deep.domain.model.TestModel">

        select
        *
        from test
        where id = #{id}
    </select>
</mapper>
